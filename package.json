{
  "name": "crass",
  "version": "0.12.3",
  "description": "A CSS utility library for JS",
  "main": "dist/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "build": "babel src -d dist",
    "prepublish": "npm run build",
    "test": "mocha --recursive --reporter=dot",
    "grammar": "jison src/grammar.y --outfile=src/grammar.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mattbasta/crass.git"
  },
  "bin": {
    "crass": "./bin/crass"
  },
  "keywords": [
    "css3",
    "css",
    "parser",
    "minifier"
  ],
  "author": "Matt Basta",
  "license": "MIT",
  "dependencies": {
    "color-convert": "^1.7.0",
    "strong-data-uri": "^1.0.4",
    "svgo": "^0.7.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "browserify": "^5.9.1",
    "jison": "^0.4.15",
    "mocha": "2.2.x"
  },
  "engines": {
    "node": ">=6.11.2"
  }
}
